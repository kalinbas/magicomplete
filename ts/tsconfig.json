        "./src/tokens/TokenBase.ts",
        "./src/util/ArrayUtil.ts",
        "./src/util/StringUtil.ts",
        "./test/phrase/PhraseTest.ts",
        "./test/ServiceTest.ts",
        "./test/tokens/StringTokenTest.ts",
        "./test/util/ArrayUtilTest.ts",
        "./typings/assertion-error/assertion-error.d.ts",
